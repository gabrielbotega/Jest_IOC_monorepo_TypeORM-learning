{
  "name": "trainingjest",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "start:dev": "nodemon --watch src/ --exec npx ts-node -r tsconfig-paths/register src/index.ts -e ts",
